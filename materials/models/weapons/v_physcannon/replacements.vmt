					"$halflambert" "1"

					"$tempvar" "0"

					"$commadd_phongexponent" "20"
					"$commadd_phongboost" 	"22.5"

					"$basemapalphaphongmask" "1"
					
					"$rimlight" "3"						
					"$rimlightexponent" "28"				
					"$rimlightboost" "3"

					// Cloaking
					"$cloakPassEnabled" "1"
					"$sheenPassEnabled" "1"

					"$yellow" "0"

					"Proxies"
					{
						"AnimatedWeaponSheen"
						{
							"animatedtexturevar" 		"$sheenmapmask"
							"animatedtextureframenumvar" 	"$sheenmapmaskframe"
							"animatedtextureframerate" 		"40"
						}
						"YellowLevel"
						{
							"resultVar" "$yellow"
						}
						"Multiply"
						{
							"srcVar1" "$color2"
							"srcVar2" "$yellow"
							"resultVar" "$color2"
						}
					}
				}
			}
		}
